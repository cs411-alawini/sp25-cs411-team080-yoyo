<!-- Views/FilterResults.ejs -->
<h2>Search Results</h2>
<% if (results.length === 0) { %>
  <p>No matching cars found.</p>
<% } else { %>
  <ul>
    <% results.forEach(car => { %>
      <li>
        <strong><%= car.Make %> <%= car.Model %></strong> - $<%= car.CarPrice %> - <%= car.ManufactureYear %>
      </li>
    <% }) %>
  </ul>
<% } %>
